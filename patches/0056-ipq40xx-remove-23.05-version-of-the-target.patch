-		val |= QCA807X_LED_BT_ON_EN_2;
-
-		led_config = true;
-	}
-
-	if (led_config)
-		return phy_write_mmd(phydev, MDIO_MMD_AN, QCA807X_MMD7_LED_1000N_1, val);
