-		ret = regmap_clear_bits(priv->psgmii, PSGMIIPHY_MODE_CONTROL,
-					PSGMIIPHY_MODE_ATHR_CSCO_MODE_25M);
-		ret = regmap_write(priv->psgmii, PSGMIIPHY_TX_CONTROL,
-				   PSGMIIPHY_TX_CONTROL_MAGIC_VALUE);
-
-		priv->psgmii_calibrated = true;
-
-		return ret;
