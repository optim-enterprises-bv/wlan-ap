-static int
-qca8k_test_dsa_port_for_errors(struct qca8k_priv *priv, struct phy_device *phy,
-			       int port, int test_phase)
-{
-	int res = 0;
-	const int test_pkts_num = QCA8075_PKT_GEN_PKTS_COUNT;
-
-	if (test_phase == 1) { /* start test preps */
