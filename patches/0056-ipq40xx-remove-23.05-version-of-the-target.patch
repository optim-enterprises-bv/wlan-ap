-static void
-qca8k_start_phy_pkt_gen(struct phy_device *phy)
-{
-	/* start traffic gen */
-	phy_write_mmd(phy, MDIO_MMD_AN, QCA8075_MMD7_PKT_GEN_CTRL,
-		      QCA8075_MMD7_PKT_GEN_START | QCA8075_MMD7_PKT_GEN_INPROGR);
