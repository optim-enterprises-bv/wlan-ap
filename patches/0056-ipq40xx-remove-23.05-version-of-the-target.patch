-	uint32_t i;
-
-	switch (stringset) {
-	case ETH_SS_STATS:
-		for (i = 0; i < ARRAY_SIZE(ipqess_stats); i++) {
-			memcpy(p, ipqess_stats[i].string,
-			       min((size_t)ETH_GSTRING_LEN,
-			       strlen(ipqess_stats[i].string) + 1));
-			p += ETH_GSTRING_LEN;
-		}
-		break;
-	}
