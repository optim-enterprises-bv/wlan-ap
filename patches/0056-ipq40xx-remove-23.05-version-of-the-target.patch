-	int val;
-
-	val = phy_read_mmd(phydev, MDIO_MMD_AN, QCA807X_MMD7_LED_1000N_1);
-	if (val < 0)
-		return val;
-
-	if (of_property_read_bool(node, "qcom,single-led-1000")) {
-		val |= QCA807X_LED_TXACT_BLK_EN_2;
-		val |= QCA807X_LED_RXACT_BLK_EN_2;
-		val |= QCA807X_LED_GT_ON_EN_2;
-
-		led_config = true;
-	}
-
