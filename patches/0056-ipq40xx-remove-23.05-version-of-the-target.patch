-static void
-qca8k_switch_port_loopback_on_off(struct qca8k_priv *priv, int port, int on)
-{
-	u32 val = QCA8K_PORT_LOOKUP_LOOPBACK;
-
-	if (on == 0)
-		val = 0;
-
-	qca8k_rmw(priv, QCA8K_PORT_LOOKUP_CTRL(port),
-		  QCA8K_PORT_LOOKUP_LOOPBACK, val);
