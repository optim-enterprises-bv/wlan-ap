-
-		if (ess->tx_ring[i].hw_desc)
-			continue;
-
-		for (j = 0; j < IPQESS_TX_RING_SIZE; j++) {
-			struct ipqess_buf *buf = &ess->tx_ring[i].buf[j];
-
-			ipqess_tx_unmap_and_free(&ess->pdev->dev, buf);
-		}
-
-		ess->tx_ring[i].buf = NULL;
-	}
