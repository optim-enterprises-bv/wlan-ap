-				     struct ethtool_stats *stats,
-				     uint64_t *data)
-{
-	struct ipqess *ess = netdev_priv(netdev);
-	u32 *essstats = (u32 *)&ess->ipqessstats;
-	int i;
-
-	spin_lock(&ess->stats_lock);
-
-	ipqess_update_hw_stats(ess);
